{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "id": "CarouselImg",
    "type": "object",
    "properties": {
        "imgList": {
            "type": "array",
            "minItems": 1,
            "maxItems": 5,
            "uniqueItems": true,
            "items": {
                "type": "object",
                "properties": {
                    "imgUrl": {
                        "type": "string",
                        "format": "uri"
                    },
                    "imgLink": {
                        "type": "string",
                        "format": "uri"
                    },
                    "time": {
                        "title": "Time",
                        "type": "string",
                        "format": "time"
                    },
                    "dateTime": {
                        "title": "Date Time",
                        "type": "string",
                        "format": "date-time"
                    },
                    "dateTimeNumber": {
                        "title": "Date Time",
                        "type": "number",
                        "format": "date-time"
                    },
                    "date": {
                        "title": "Date",
                        "type": "string",
                        "format": "date"
                    },
                    "dateNumber": {
                        "title": "Date Number",
                        "type": "number",
                        "format": "date"
                    }
                },
                "required": [
                    "imgUrl",
                    "imgLink"
                ]
            }
        }
    },
    "additionalProperties": false
}
